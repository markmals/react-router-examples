        "@remix-run/react": "^1.16.0",
        "cross-env": "^7.0.3",
        "isbot": "^3.6.10",
        "match-sorter": "^6.3.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "sort-by": "^1.2.0"
    },
    "devDependencies": {
        "@remix-run/dev": "^1.16.0",
        "@remix-run/eslint-config": "^1.16.0",
        "@types/node": "^18.16.5",
        "@types/react": "^18.2.6",
        "@types/react-dom": "^18.2.4",
        "@types/sort-by": "^1.2.0",
        "chokidar": "^3.5.3",
        "dotenv": "^16.0.3",
        "eslint": "^8.40.0",
